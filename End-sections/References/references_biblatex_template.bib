@article{gruter_honeybee_2009,
	title = {The honeybee waggle dance: can we follow the steps?},
	volume = {24},
	issn = {0169-5347},
	url = {http://www.sciencedirect.com/science/article/pii/S0169534709000779},
	doi = {10.1016/j.tree.2008.12.007},
	shorttitle = {The honeybee waggle dance},
	abstract = {The honeybee (Apis mellifera) waggle dance, whereby dancing bees communicate the location of profitable food sources to other bees in the hive, is one of the most celebrated communication behaviours in the animal world. Dance followers, however, often appear to ignore this location information, the so-called dance language, after leaving the nest. Here we consider why foragers follow dances and discuss the function of the dance as a multicomponent signal. We argue that the ‘dance language’ is just one information component of the waggle dance and that the two terms should not be used synonymously. We propose that location information is often backup information to be used in case information acquired directly from the environment about food sources is not available, is of poor quality or is associated with unprofitable food sources.},
	pages = {242--247},
	number = {5},
	journaltitle = {Trends in Ecology \& Evolution},
	shortjournal = {Trends in Ecology \& Evolution},
	author = {Grüter, Christoph and Farina, Walter M.},
	urldate = {2018-01-30},
	date = {2009-05-01},
	note = {00105},
	file = {ScienceDirect Full Text PDF:/Users/janek/Zotero/storage/L7PF88PI/Grüter and Farina - 2009 - The honeybee waggle dance can we follow the steps.pdf:application/pdf;ScienceDirect Snapshot:/Users/janek/Zotero/storage/2I6FJ634/S0169534709000779.html:text/html}
